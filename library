pragma solidity ^0.4.15;

library intExtanded{
    
    function increent (uint _self) returns (uint){
        return _self+1;
    }
    function decreent (uint _self) returns (uint){
        return _self - 1;
    }
    function increentByValue(uint _self, uint _value) returns (uint){
        return _self + _value;
    }
    function decreentByValue (uint _self, uint _value) returns(uint){
        return _self - _value;
    }
}